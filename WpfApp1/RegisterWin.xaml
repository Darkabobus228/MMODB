<Window x:Class="WpfApp1.RegisterWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="RegisterWin" Height="450" Width="300">
    <Grid>
        <StackPanel Margin="5 50 5 0">
            <TextBox Name="tb_loginLog"/>
            <TextBox Name="tb_loginPass" Margin="0 5 0 0"/>
            <Button Name="bt_login" Content="Login" Margin="0 5 0 0" Click="bt_login_Click"/>
        </StackPanel>
    </Grid>
</Window>
