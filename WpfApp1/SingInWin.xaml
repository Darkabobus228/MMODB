<Window x:Class="WpfApp1.SingInWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="SingInWin" Height="450" Width="300">
    <Grid>
        <StackPanel Margin="5 50 5 0">
            <TextBox Name="tb_login" Margin="0 0 0 5"/>
            <TextBox Name="tb_password" Margin="0 0 0 5"/>
            <Button Name="bt_signIn" Content="Sign In" Click="bt_signIn_Click"/>
            <Button Name="bt_register" Content="Register" Margin="0 10 0 0" Click="bt_register_Click"/>
        </StackPanel>
    </Grid>
</Window>
